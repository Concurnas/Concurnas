import java.lang.*

val year = 1900
var leap = true

if(year mod 4 == 0){
  if(year mod 100 == 0){
    if(year mod 400 == 0){
      leap = true
    }
  }
  else { leap = true }
}
else { leap = false }

if ( leap == true ){ System.out.println( year + " is a leap year.") } 
else { System.out.println( year + "is not a leap year") }
