//##3. more complex

def matcher(a int){
	res = ""
	
	match(a){
		case(10){
			res  = "ten"
		}
		case(==9){
			res  = "explicit 9"
		}
		case(2 or 3){
			res = "or case"
		}
		case(>2 and >5 and <==10){
			res = "gt 5"
		}
		case(>3 and <==10){
			res = "gt three not ten"
		}
		case(>11){
			res = "gt to 11"
		}
		else{
			res = "something else" + a
		}
	}
	
	res
}


def doings(){
	"" + [matcher(2), matcher(10), matcher(9), matcher(4),  matcher(6), matcher(20), matcher(-5)] 
}

