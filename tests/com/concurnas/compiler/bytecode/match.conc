//##1. simple match
def matcher(a int){
	res = ""
	
	match(a){
		case(5){
			res  = "five"
		}
		case(1 or 2){
			res  = "1 or 2"
		}
		case(>7){
			res  = "gt 7"
		}
		else{
			res="?"
		}
	}
	
	res
}

def range(x int){
	for(n=0; n < x; n++) { n }
}

def doings(){
	"" + for(x in range(10)){ matcher(x)}//[matcher(2), matcher(10)]
}

